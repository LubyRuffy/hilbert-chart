{
  "name": "hilbert-chart",
  "version": "1.3.0",
  "description": "A hilbert space-filling curve D3 chart for representing one-dimensional lengths on a two-dimensional space.",
  "unpkg": "dist/hilbert-chart.min.js",
  "main": "dist/hilbert-chart.common.js",
  "module": "dist/hilbert-chart.module.js",
  "keywords": [
    "hilbert",
    "space filling",
    "curve",
    "d3",
    "chart"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "homepage": "https://github.com/vasturiano/hilbert-chart",
  "repository": {
    "type": "git",
    "url": "https://github.com/vasturiano/hilbert-chart.git"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "watch": "rollup -c -w",
    "minify": "uglifyjs dist/hilbert-chart.js -o dist/hilbert-chart.min.js -c -m --comments '/Version/'",
    "build-module": "rollup -c rollup.config.module.js",
    "prepublishOnly": "npm run build && npm run minify && npm run build-module"
  },
  "dependencies": {
    "accessor-fn": "^1.2.2",
    "d3-axis": "^1.0.12",
    "d3-hilbert": "^1.1.0",
    "d3-scale": "^1.0.7",
    "d3-selection": "^1.3.2",
    "d3-tip": "0.7.1",
    "d3-zoom": "^1.7.3",
    "gsap": "1.20.4",
    "heatmap.js": "^2.0.5",
    "kapsule": "^1.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "postcss-nested": "^4.1.0",
    "postcss-simple-vars": "^5.0.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-watch": "^4.3.1",
    "uglify-js": "^3.4.9"
  }
}
