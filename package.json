{
  "name": "hilbert-chart",
  "version": "0.2.4",
  "description": "A hilbert space-filling curve D3 chart for representing one-dimensional lengths on a two-dimensional space.",
  "keywords": [
    "hilbert",
    "space filling",
    "curve",
    "d3",
    "chart"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "license": "GPL-3.0",
  "main": "dist/hilbert-chart.js",
  "jsnext:main": "src/index",
  "homepage": "https://github.com/vasturiano/hilbert-chart",
  "repository": {
    "type": "git",
    "url": "https://github.com/vasturiano/hilbert-chart.git"
  },
  "scripts": {
    "build": "rm -rf dist && mkdir dist && rollup -c --banner \"$(preamble)\"",
    "watch": "rollup -c -w",
    "minify": "uglifyjs dist/hilbert-chart.js -o dist/hilbert-chart.min.js -c -m --preamble \"$(preamble)\"",
    "prepublish": "npm run build && npm run minify"
  },
  "dependencies": {
    "d3": "^4.2",
    "d3-hilbert": "^0.2.4",
    "d3-tip": "^0.7"
  },
  "devDependencies": {
    "package-preamble": "^0.0.2",
    "postcss-nested": "^1.0",
    "postcss-simple-vars": "^3.0",
    "rollup": "^0.36",
    "rollup-plugin-commonjs": "^5.0",
    "rollup-plugin-node-resolve": "^2.0",
    "rollup-plugin-postcss": "^0.2",
    "rollup-watch": "^2.5",
    "uglify-js": "^2.7"
  }
}
